{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "todolite/src",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "todolite"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "todolite"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart format .",
        "cwd": "todolite"
      }
    },
    "buildRunner": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "flutter pub get && flutter packages pub run build_runner build --delete-conflicting-outputs",
            "forwardAllArgs": false
          }
        ],
        "cwd": "todolite"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "flutter test --coverage",
            "forwardAllArgs": false
          },
          {
            "command": "../codecov -t ${CODECOV_TOKEN} -c -F todo0 -f coverage/lcov.info",
            "forwardAllArgs": false
          }
        ],
        "cwd": "todolite",
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "buildRunner",
          "projects": "self"
        }
      ]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze && dart format . --set-exit-if-changed",
        "cwd": "todolite"
      },
      "dependsOn": [
        {
          "target": "buildRunner",
          "projects": "self"
        }
      ]
    },
    "dartCodeMetricsAnalyze": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dcm analyze lib --reporter=github --ci-key={args.dcmCiKey} --email={args.dcmLicenseEmail}",
        "cwd": "todolite"
      },
      "dependsOn": [
        {
          "target": "buildRunner",
          "projects": "self"
        }
      ]
    },
    "dartCodeMetricsCheckUnusedFiles": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dcm check-unused-files lib --monorepo --ci-key={args.dcmCiKey} --email={args.dcmLicenseEmail}",
        "cwd": "todolite"
      },
      "dependsOn": [
        {
          "target": "buildRunner",
          "projects": "self"
        }
      ]
    },
    "assemble": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter assemble",
        "cwd": "todolite"
      }
    },
    "attach": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter attach",
        "cwd": "todolite"
      }
    },
    "drive": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter drive",
        "cwd": "todolite"
      }
    },
    "genL10n": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "todolite"
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter install",
        "cwd": "todolite"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter run",
        "cwd": "todolite"
      }
    },
    "buildAar": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "todolite"
      }
    },
    "buildAndroid": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build {args.artifactType} --flavor={args.flavor} --target=lib/main_{args.flavor}.dart --build-number={args.buildNumber} {args.extraOpts}",
        "buildNumber": "1",
        "cwd": "todolite"
      },
      "configurations": {
        "development": {
          "flavor": "development",
          "artifactType": "apk",
          "extraOpts": "--no-tree-shake-icons"
        },
        "production": {
          "flavor": "production",
          "artifactType": "appbundle",
          "extraOpts": ""
        }
      },
      "dependsOn": [
        {
          "target": "buildRunner",
          "projects": "self"
        }
      ]
    },
    "buildBundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "todolite"
      }
    },
    "buildIos": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "todolite"
      }
    },
    "buildIosFramework": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "todolite"
      }
    },
    "buildIpa": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ipa --flavor={args.flavor} --target=lib/main_{args.flavor}.dart --build-number={args.buildNumber} --export-options-plist=ios/Runner/{args.flavor}/ExportOptions.plist {args.extraOpts}",
        "buildNumber": "1",
        "cwd": "todolite",
        "forwardAllArgs": false
      },
      "configurations": {
        "development": {
          "flavor": "development",
          "extraOpts": "--no-tree-shake-icons"
        },
        "production": {
          "flavor": "production",
          "extraOpts": ""
        }
      },
      "dependsOn": [
        {
          "target": "buildRunner",
          "projects": "self"
        },
        {
          "target": "syncCodeSigning",
          "projects": "self"
        }
      ]
    },
    "deployAndroid": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bundle exec fastlane android deploy_to_firebase",
            "forwardAllArgs": false
          }
        ],
        "cwd": "todolite/android"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "buildAndroid",
          "projects": "self",
          "params": "forward"
        }
      ]
    },
    "deployAndroidToStore": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bundle exec fastlane android deploy_to_store",
            "forwardAllArgs": false
          }
        ],
        "cwd": "todolite/android"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "buildAndroid",
          "projects": "self",
          "params": "forward"
        }
      ]
    },
    "deployIos": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bundle exec fastlane deploy api_key_path:{args.apiKeyPath} ipa:'{args.ipa}' app_identifier:{args.appIdentifier}",
            "forwardAllArgs": false
          }
        ],
        "cwd": "todolite/ios",
        "apiKeyPath": "${APPLE_STORE_CONNECT_API_KEY}"
      },
      "configurations": {
        "development": {
          "ipa": "../build/ios/ipa/EDI [D].ipa",
          "appIdentifier": "jp.andpad.ios.develop.edi"
        },
        "production": {
          "ipa": "../build/ios/ipa/EDI.ipa",
          "appIdentifier": "jp.andpad.ios.production.edi"
        }
      },
      "dependsOn": [
        {
          "target": "buildIpa",
          "projects": "self",
          "params": "forward"
        }
      ]
    },
    "syncCodeSigning": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bundle exec fastlane my_match type:{args.type}",
            "forwardAllArgs": false
          }
        ],
        "cwd": "todolite/ios",
        "type": "development"
      },
      "configurations": {
        "development": {
          "type": "appstore"
        },
        "production": {
          "type": "appstore"
        }
      }
    },
    "fixFlutterVersion": {
      "executor": "nx:run-commands",
      "inputs": [
        {
          "runtime": "cat .env"
        }
      ],
      "options": {
        "command": "echo 'FLUTTER_VERSION=${env.FLUTTER_VERSION}' >> $GITHUB_ENV",
        "cwd": "todolite"
      }
    }
  },
  "tags": []
}
